{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run check-size",
    "check-size": "node -e \"const fs=require('fs');const stats=fs.statSync('dist/assets/index-*.js');console.log('Bundle size:',Math.round(stats.size/1024),'KB');if(stats.size>204800)console.warn('Warning: Bundle exceeds 200KB target');\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:api": "openapi --input ../shared/openapi/swagger.json --output ./src/lib/api --client axios --useOptions --useUnionTypes",
    "prebuild": "npm run generate:api"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.5",
    "axios": "^1.12.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "openapi-typescript-codegen": "^0.29.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
